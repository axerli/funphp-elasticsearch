{
    "name": "funphp/elasticsearch",
    "description": "An Elasticsearch query package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Funphp\\Elasticsearch\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Axerli"
        }
    ],
    "require": {
        "php": ">=7.4",
        "elasticsearch/elasticsearch": "^7.8.0",
        "laravel/framework": ">=6.0"
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Funphp\\Elasticsearch\\ElasticsearchServiceProvider"
            ]
        }
    }
}
